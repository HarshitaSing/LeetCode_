//using min_heap for ascending sort
	public int[] KSortedArray(int nums[],int k)
	{
		PriorityQueue<Integer> min_heap = new PriorityQueue<>();
		int index=0;
		
		for(int i=0;i<k;i++)
		{
			min_heap.add(nums[i]);
		}
		
		for(int i=k;i<nums.length;i++)
		{
			min_heap.add(nums[i]);
			nums[index] = min_heap.poll();
			++index;
		}
		
		while(min_heap.size() > 0)
		{
			min_heap.poll();
			++index;
		}
		
		return nums;
	}
